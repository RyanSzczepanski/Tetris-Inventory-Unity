  functions:
  - name: GetAllSubInventories
    body: "List<Vector2Int> subInventories = new List<Vector2Int>();\t\n\t\nif (!m_SubInventoryArrangements.IsLeaf)\t\n{\t\n\tforeach
      (SubInventoryArrangement child in m_SubInventoryArrangements.childArrangements)\t\n\t{\t\n\t\tsubInventories.AddRange(GetAllSubInventories());\t\n\t}\t\n}\t\nif
      (m_SubInventoryArrangements.HasSubInventory)\t\n{\n\tsubInventories.Add(m_SubInventoryArrangements.subInventorySize);\t\n}\t\nreturn
      subInventories.ToArray();"
    type: 'Vector2Int[]'
    accessibility: 0
    isOverride: 0
  - name: GetStorageSlotsCount
    body: "int storage = 0;\t\nforeach (Vector2Int subInventory in GetAllSubInventories())\t\n{\t\n\tstorage
      += subInventory.x * subInventory.y;\t\n}\t\nreturn storage;"
    type: int
    accessibility: 0
    isOverride: 0
